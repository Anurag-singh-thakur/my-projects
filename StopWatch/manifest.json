{
    "manifest_version": 2, 
    "name": "Stopwatch Extension",
    "version": "1.0",
    "description": "A simple stopwatch for Chrome",
    "permissions": [], 
    "content_scripts": [
      {
        "matches": ["<all_urls>"], 
        "js": ["content.js"]  
      }
    ],
    "browser_action": {
      "default_popup": "index.html"
    }
  }
  